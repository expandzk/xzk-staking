{
  "name": "@mystikonetwork/staking-rewards-abi",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/mystikonetwork/mystiko-staking-rewards",
    "directory": "packages/abi"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "main": "build/cjs/index.js",
  "module": "build/esm/index.js",
  "types": "build/cjs/index.d.ts",
  "files": [
    "build"
  ],
  "author": "Mystiko Network",
  "license": "MIT",
  "scripts": {
    "lint": "yarn prettier:check",
    "prettier:check": "prettier --check \"./**/*.{js,ts}\" --config .prettierrc",
    "format": "prettier --write \"./**/*.{js,ts}\" --config .prettierrc",
    "generate": "rm -rf src/typechain/staking-rewards && typechain --target ethers-v5 --out-dir src/typechain/staking-rewards '../contracts/artifacts/{contracts,lib/openzeppelin-contracts/contracts/token,lib/openzeppelin-contracts/contracts/access}/!(build-info)/**/+([a-zA-Z0-9_]).json'",
    "build": "rm -rf build && yarn build:contracts && yarn build:cjs && yarn build:esm",
    "build:cjs": "tsc -p ./tsconfig.json",
    "build:esm": "tsc -p ./tsconfig.esm.json",
    "build:contracts": "cd ../contracts && yarn build",
    "clean": "rm -rf src/typechain && rm -rf build",
    "test": ""
  },
  "devDependencies": {
    "@mystikonetwork/staking-rewards-contracts": "1.0.0",
    "@tsconfig/recommended": "^1.0.1",
    "@typechain/ethers-v5": "^11.1.2",
    "prettier": "3.2.4",
    "typechain": "^8.3.2",
    "typescript": "5.1.6"
  },
  "dependencies": {
    "@ethersproject/abi": "^5.5.3",
    "@ethersproject/providers": "^5.5.3",
    "ethers": "^5.5.3"
  }
}